    "Mappings": {
      "Sci-Fi": "Science Fiction",
      "SciFi": "Science Fiction",
      "Sci Fi": "Science Fiction",
      "Doc": "Documentary",
      "Docu": "Documentary",
      "Anime": "Animation",
      "Cartoon": "Animation",
      "R&B": "Rhythm and Blues",
      "Hip-Hop": "Hip Hop",
      "Rap": "Hip Hop"
    }
  },
  "AllowedHosts": "*"
}
